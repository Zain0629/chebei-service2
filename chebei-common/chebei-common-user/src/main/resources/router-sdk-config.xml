                 reconnInterval="3000"
                 localServerName="chebei-common-user" localServerAlias="chebei-common-user" callBackTime="100000"
                 enmCompress="ZLIB"/>
    <parents>
        <parent parentName="router">
            <members>
                <!-- 本地环境 -->
                <member no="1" address="localhost" port="8088" poolSize="3" safeLevel="NONE"/>
                <!-- 开发环境 -->
                <!-- <member no="1" address="172.16.3.243" port="3300" poolSize="3" safeLevel="NONE" /> -->
                <!-- 生产环境 -->
                <!-- <member no="1" address="47.99.65.120" port="3300" poolSize="1" safeLevel="NONE" /> -->
            </members>
        </parent>
    </parents>
</ams-client-config>